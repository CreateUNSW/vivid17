// recalculate distance array if center changes
void changeCentre(int centre) {
  if(centre != oldCentre) {
    oldCentre = centre;
    if(dist != NULL) delete[] dist;
    dist = g->calcDist(centre);
    maxDistance = g->maxDist(dist);
  }
}

void changeColor() {
    fadeSpeed = FADE_AMOUNT;
    radialIndex = 0;
    swipeIndex = 0;
}
