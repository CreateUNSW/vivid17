// Picks randome color gradient to white
void colorToWhite(bool *wing, int centre, bool change) {
  
  changeCentre(centre);
  int hue;
  if(change) {
    hue = rand() % 250;
    for(int i = 0; i < NUM_CRYSTALS; i++) {
      if(wing == NULL || wing[i]) {
        crystalHSV(i, hue,
                  (1 - (dist[i] / (double)maxDistance)) * 255, // Saturation
                  255);//150); // Brightness
      } else {
        // Turns off other crystals
        crystalHSV(i, 0, 0, 0);
      }
    }
  }
}

